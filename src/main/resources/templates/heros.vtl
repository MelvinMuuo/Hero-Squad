<div class="header">
  <h1 class="header1">Hero list</h1>
</div>
#if ($heros)
  <ul>
    #foreach ($hero in $heros)
      <li><a href="/heros/$hero/$hero.getId()">$hero.getName()</a></li>
    #end
  </ul>
#else
  <p>No heros!</p>
#end
</div class="line1">
  <a href="/heros/new">Add new Hero</a>
</div>
